; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="space_game_remake"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

DebugPrint="*res://debugs/DEBUG_PRINT.gd"

[display]

window/size/viewport_width=480
window/size/viewport_height=854
window/size/resizable=false
window/stretch/mode="viewport"
window/handheld/orientation=1

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

2d_physics/layer_1="environment"
2d_physics/layer_2="player_hitboxes"
2d_physics/layer_3="enemy_hitboxes"
2d_physics/layer_4="player_collision"
2d_physics/layer_5="enemy_collision"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
